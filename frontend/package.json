{
  "name": "ffxiv-crafting",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others \"less-watch-compiler --config less-watcher.config.json\" \"next dev\"",
    "build": "less-watch-compiler --config less-watcher.config.json --run-once && next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "next": "^14.0.0",
    "react": "latest",
    "react-dom": "latest",
    "sharp": "^0.32.6"
  },
  "devDependencies": {
    "@types/node": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "concurrently": "^8.2.1",
    "eslint": "latest",
    "eslint-config-next": "latest",
    "less": "^4.2.0",
    "less-watch-compiler": "^1.16.3",
    "ts-auto-guard": "^4.1.3",
    "typescript": "latest"
  }
}